    "compilerOptions": {
        "target": "es2017",
        "strict": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "module": "CommonJS",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "../../../../dist"
